; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="RotateMaze"
run/main_scene="res://Maze.tscn"
config/icon="res://icon.png"

[editor_plugins]

enabled=PoolStringArray( "res://addons/DiagramsGenerator/plugin.cfg", "res://addons/godot-plugin-refresher/plugin.cfg" )

[layer_names]

2d_physics/layer_1="Player_1"
2d_physics/layer_2="Player_2"
2d_physics/layer_3="Player_3"
2d_physics/layer_4="Player_4"
2d_physics/layer_5="Treasure"
2d_physics/layer_6="Ground"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
